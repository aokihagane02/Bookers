<%# @books.each do |book| %>
    <%#= book.title %>
    <%#= book.body %>
    <%#= link_to 'Show', "/books/#{book.id}" %>
    <%#= link_to 'Edit', "/books/#{book.id}/edit" %>
    <%#= link_to 'Destroy', "/books/#{book.id}", method: :destroy %>
<%# end %>


<h2>New Book</h2>
<%= form_with model:@book, local: true do |f| %>
  <h4>Title</h4>
  <%= f.text_field :title %>
  <h4>Body</h4>
  <%= f.text_area :body %>
  <%= f.submit 'Create Book' %>
<% end %>